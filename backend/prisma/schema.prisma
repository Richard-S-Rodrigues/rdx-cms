generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" 
}

model User {
  id           String @id @default(uuid())
  first_name   String
  last_name    String
  email        String
  password     String

  @@map("users")

}

model ResetPasswordToken {
  id          String @id @default(uuid())
  token       String
  user_id     String
  created_at  DateTime @default(now())

  @@map("resetPasswordTokens")
}
